doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Exemplo de Pug com Navbar e Carrossel
    link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css")
  body
    header
      nav.navbar.navbar-expand-lg.navbar-light.bg-light
        .container
          a.navbar-brand(href="#") Navbar
          button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarNav", aria-controls="navbarNav", aria-expanded="false", aria-label="Toggle navigation")
            span.navbar-toggler-icon
          .collapse.navbar-collapse#navbarNav
            ul.navbar-nav.ml-auto
              li.nav-item
                a.nav-link(href="#") Home
              li.nav-item
                a.nav-link(href="#") About
              li.nav-item
                a.nav-link(href="#") Services
              li.nav-item
                a.nav-link(href="#") Contact

    .container.mt-4
      .row
        .col-md-8
          .carousel.slide(data-ride="carousel", id="carouselExampleSlidesOnly")
            .carousel-inner
              .carousel-item.active
                img.d-block.w-100(src="data:image/jpeg;base64,", alt="First slide")
              .carousel-item
                img.d-block.w-100(src="https://uniaodooeste.sc.gov.br/uploads/sites/434/2022/09/3365489.jpeg", alt="Second slide")
              .carousel-item
                img.d-block.w-100(src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTVRqT7tNc7TsetY9qhl6eUlARReW-nbXV7fw&s", alt="Third slide")
            a.carousel-control-prev(href="#carouselExampleSlidesOnly", role="button", data-slide="prev")
              span.carousel-control-prev-icon(aria-hidden="true")
              span.sr-only Previous
            a.carousel-control-next(href="#carouselExampleSlidesOnly", role="button", data-slide="next")
              span.carousel-control-next-icon(aria-hidden="true")
              span.sr-only Next

    footer.footer.bg-light.py-3
      .container
        p.text-center © 2024 Todos os direitos reservados.

  script(src="https://code.jquery.com/jquery-3.5.1.slim.min.js")
  script(src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js")
  script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js")
